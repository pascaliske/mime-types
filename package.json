{
  "name": "@pascaliske/mime-types",
  "description": "Simple wrapper for node-mime package.",
  "version": "1.0.2",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/mime-types.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/mime-types/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts",
    "watch": "rollup --config rollup.config.ts --watch",
    "postbuild": "tsc --declaration --outDir dist --emitDeclarationOnly --resolveJsonModule src/index.ts",
    "lint": "tslint --project tsconfig.json --fix",
    "test": "nyc ava tests/**/*.test.js",
    "check": "snyk test",
    "format": "prettier --write \"src/**/*.ts\"",
    "changelog": "standard-changelog --release-count 1"
  },
  "dependencies": {
    "mime": "^2.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "ava": "^1.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "nyc": "^13.1.0",
    "prettier": "^1.16.3",
    "rollup": "^1.9.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.20.1",
    "snyk": "^1.124.0",
    "standard-changelog": "^2.0.6",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.3.1"
  }
}

{
  "name": "@pascaliske/mime-types",
  "description": "Simple wrapper for node-mime package.",
  "version": "1.0.1",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/mime-types.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/mime-types/issues"
  },
  "main": "dist/index.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "nodemon --exec \"rm -rf dist typings && yarn run build\" --watch src --ext ts",
    "lint": "tslint -p tsconfig.json --fix",
    "test": "ava tests/**/*.test.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tslint -p tsconfig.json --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "mime": "^2.3.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "husky": "^1.0.0",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.13.7",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^2.9.2"
  }
}
